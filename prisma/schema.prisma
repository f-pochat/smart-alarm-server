// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alarm {
  id    String  @id @default(uuid())
  days Json
  time DateTime
  createdAt  DateTime
  isActive Boolean
  deviceId String
  name String
}

